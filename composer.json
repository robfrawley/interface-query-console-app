{
    "name": "src-run/interface-query-console-app",
    "license": "MIT",
    "type": "project",
    "homepage": "https://src.run/interface-query-console-app",
    "description": "Retrieve requested data values for the specified wireless interfaces.",
    "support": {
        "email": "rmf@src.run",
        "issues": "https://src.run/interface-query-console-app/issues",
        "source": "https://src.run/interface-query-console-app/git"
    },
    "keywords": [
        "interface-query-console-app",
        "interface",
        "query",
        "console",
        "app",
        "rmf",
        "src-run"
    ],
    "authors": [
        {
            "name": "Rob Frawley 2nd",
            "email": "rmf@src.run",
            "homepage": "https://src.run/rmf",
            "role": "Project Lead"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-posix": "*",
        "ramsey/uuid": "^3.8",
        "src-run/cocoa-var-dumper-library": "^0.2.0",
        "symfony/console": "^4.2",
        "symfony/dotenv": "^4.2",
        "symfony/finder": "^4.2",
        "symfony/process": "^4.2",
        "symfony/yaml": "^4.2"
    },
    "require-dev": {
        "codacy/coverage": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "php-coveralls/php-coveralls": "^2.0",
        "symfony/debug": "^3.4|^4.1",
        "symfony/phpunit-bridge": "^4.2",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
